    .dark .horizontal-card:hover {
      border: 1px solid rgba(168, 85, 247, 0.4);
      box-shadow:
        0 20px 40px -10px rgba(0, 0, 0, 0.6),
        0 8px 16px -4px rgba(168, 85, 247, 0.2),
        inset 0 1px 0 rgba(255, 255, 255, 0.1);
    }

    /* Typography improvements */
    h1,
    h2,
    h3,
    h4,
    h5,
    h6 {
      @apply tracking-wide;
    }

    /* Softer text colors */
    .text-90 {
      @apply text-black/85 dark:text-white/85;
    }
    .text-75 {
      @apply text-black/70 dark:text-white/70;
    }
    .text-65 {
      @apply text-black/60 dark:text-white/60;
    }
    .text-50 {
      @apply text-black/45 dark:text-white/45;
    }
    .text-30 {
      @apply text-black/25 dark:text-white/25;
    }
  }

